{
  "license": "MIT",
  "engines": {
    "node": "^14.11.0"
  },
  "scripts": {
    "start": "node server.js",
    "start:dev": "hof-build watch",
    "test": "npm run test:lint && npm run test:unit && npm run test:integration",
    "test:accessibility": "_mocha \"test/_accessibility/**/*.spec.js\" --exit",
    "test:acceptance": "TAGS=\"${TAGS:=@feature}\" npm run test:cucumber",
    "test:acceptance_browser": "ACCEPTANCE_WITH_BROWSER=true TAGS=\"${TAGS:=@feature}\" npm run test:cucumber",
    "test:cucumber": "cucumber-js -f @cucumber/pretty-formatter \"test/_features/**/*.feature\" --require test/_features/test.setup.js --require \"test/_features/step_definitions/**/*.js\" --tags $TAGS",
    "test:integration": "_mocha \"test/_integration/**/*.spec.js\" --exit",
    "test:lint": "eslint . --config ./node_modules/eslint-config-hof/default.js",
    "test:snyk": "snyk config set api=SNYK_TOKEN && snyk test",
    "test:unit": "nyc _mocha \"test/_unit/**/*.spec.js\"",
    "build": "hof-build",
    "postinstall": "npm run build"
  },
  "mocha": {
    "reporter": "spec",
    "require": "test/setup.js",
    "recursive": "true",
    "timeout": "6000"
  },
  "@comment dependencies": [
    "path-parse@^1.0.7 has been specified due to hof-build needing an update",
    "cookie@^0.4.1, ini@^1.3.8, kind-of@^6.0.3 have been included to update minor versions",
    "trim-newlines@^3.0.1, updated major version due to security update",
    "but these should be investigated to understand if they need updating as part of HOF dependency tree"
  ],
  "dependencies": {
    "aws-sdk": "^2.814.0",
    "cookie": "^0.4.1",
    "debug": "^4.2.0",
    "express": "^4.17.1",
    "express-healthcheck": "^0.1.0",
    "hof": "^18.3.0",
    "hof-build": "^2.0.0",
    "hof-component-date": "^1.4.0",
    "hof-model": "^3.1.0",
    "hof-theme-govuk": "^5.2.4",
    "ini": "^1.3.8",
    "kind-of": "^6.0.3",
    "libphonenumber-js": "^1.8.4",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "notifications-node-client": "^4.9.0",
    "path-parse": "^1.0.7",
    "puppeteer": "^5.5.0",
    "trim-newlines": "^3.0.1",
    "uuid": "^3.3.3",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@cucumber/cucumber": "^7.2.1",
    "@cucumber/pretty-formatter": "^1.0.0-alpha.0",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "chai-subset": "^1.6.0",
    "eslint-config-hof": "^1.0.4",
    "jquery": "^3.5.1",
    "jsdom": "^16.4.0",
    "mocha": "^8.2.0",
    "nyc": "^15.1.0",
    "pa11y": "^6.0.0",
    "playwright": "^1.11.1",
    "proxyquire": "^2.1.3",
    "reqres": "^3.0.1",
    "sinon": "^9.2.0",
    "sinon-chai": "^3.5.0",
    "snyk": "^1.620.0",
    "supertest": "^5.0.0",
    "supertest-session": "^4.1.0"
  },
  "name": "refugee-integration-loans",
  "version": "1.0.0",
  "main": "index.js",
  "keywords": [],
  "author": "",
  "description": ""
}
